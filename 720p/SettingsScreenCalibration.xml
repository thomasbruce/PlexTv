<?xml version="1.0"?>
<window id="11">
	<defaultcontrol always="true">8</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>
		<control type="label">
			<posx>100</posx>
			<include>DialogID</include>
			<label>SettingsScreenCalibration.xml</label>
		</control>
		<control type="videowindow" id="20">
			<description>videowindow</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>720</width>
			<height>576</height>
		</control>
		<control type="image">
			<width>720</width>
			<height>576</height>
			<aspectratio>stretch</aspectratio>
			<texture>CalibrateBack.png</texture>
		</control>
		<control type="mover" id="8">
			<description>top left mover</description>
			<posx>20</posx>
			<posy>20</posy>
			<texturefocus>CalibrateTopLeftFo.png</texturefocus>
			<texturenofocus>CalibrateTopLeftNF.png</texturenofocus>
		</control>
		<control type="mover" id="9">
			<description>right bottom mover</description>
			<posx>699</posx>
			<posy>499</posy>
			<texturefocus>CalibrateBottomRightFo.png</texturefocus>
			<texturenofocus>CalibrateBottomRightNF.png</texturenofocus>
		</control>
		<control type="mover" id="10">
			<description>subtitle position mover</description>
			<posx>200</posx>
			<posy>500</posy>
			<texturefocus>CalibrateSubTitlesFo.png</texturefocus>
			<texturenofocus>CalibrateSubTitlesNF.png</texturenofocus>
		</control>
		<control type="resize" id="11">
			<description>pixel aspect ratio box</description>
			<posx>0</posx>
			<posy>270</posy>
			<width>288</width>
			<height>288</height>
			<texturefocus>CalibratePixelRatioFo.png</texturefocus>
			<texturenofocus>CalibratePixelRatioNF.png</texturenofocus>
		</control>
		<control type="label" id="2">
			<description>coordinates label</description>
			<posx>0</posx>
			<posy>40</posy>
			<width>720</width>
			<align>center</align>
			<label>-</label>
			<font>20BdFont</font>
			<textcolor>FocusedTextColor</textcolor>
		</control>
		<control type="label" id="3">
			<description>help information</description>
			<posx>0</posx>
			<posy>70</posy>
			<width>720</width>
			<align>center</align>
			<label>-</label>
			<font>20BdFont</font>
			<textcolor>FocusedTextColor</textcolor>
		</control>
	</controls>
</window>
